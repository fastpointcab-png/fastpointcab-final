{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "@vercel/node@3.2.0"
    }
  },
  "builds": [
    { "src": "api/server.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/server.js" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
